package arrays;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] numeros = new int[4];
		numeros[0] = 6;
		numeros[1] = 3;
		numeros[2] = 2;
		numeros[3] = 7;

		for (int i = 0; i < numeros.length; i++) {
			System.out.println(numeros[i]);
			System.out.println(i);
		}

	}

}
